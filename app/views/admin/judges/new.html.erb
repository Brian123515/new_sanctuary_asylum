<div class="row">
    <div class="col-sm-8 col-sm-offset-4">
	<h1>Add a new Judge</h1>
	<%= form_for [:admin, @judge] do |f| %>
	    <%= f.label :first_name %>:
	    <%= f.text_field :first_name, required: true %><br />

	    <%= f.label :last_name %>:
	    <%= f.text_field :last_name, required: true %><br />

	    <%= f.submit nil, class: 'btn btn-primary' %>
	<% end %>
    </div>
</div>
